{
  "extends": "../tsconfig-base.json",
  "display": "Node 14",
  "compilerOptions": {
    "composite": true,
    "declaration": true,
    "declarationDir": "../../types/storage-dynamodb",
    "outDir": "../../lib/storage-dynamodb",
    "lib": ["es2020"],
    "module": "commonjs",
    "target": "es2020",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true
  },
  "include": ["**/*.ts"]
}